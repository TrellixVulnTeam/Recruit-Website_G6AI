<div class="banner text-center">
    <div data-aos="fade-up" data-aos-duration="1000">
        <h1>2,786 Việc Làm IT Cho Developer Chất</h1>
        <div class="search-job text-center">
            <form action="/search/jobs" method="POST" name="form-search-job" class="form-search-job1" style="    
                display: flex;
                align-items: center;
                justify-content: center;

            ">
            
                <input type="text" name="keyword"  value="" id="input-search-job" class="search-form__keyword-input height-45 border-radius4 mr-4" autocomplete="off"
                            placeholder="Tìm kiếm theo kỹ năng, chức vụ, công ty..." data-search--autocomplete-target="query" data-action="search--autocomplete#toggleClearButton">
                <select id="form-search-location" name="location" class="form-search-combobox height-45 border-radius4 mr-4">
                                    <option value="Ho Chi Minh">Ho Chi Minh</option>
                                    <option value="Ha Noi">Ha Noi</option>
                                    <option value="Da Nang">Da Nang</option>
                </select>
                

                <button type="submit" id="btn-search-job" class="search-form-submit">
                                <div class="search-form-submit-icon">
                                    <i class="fa-solid fa-magnifying-glass mr-2"></i> Tìm kiếm
                                </div>
                </button>
            </form>
        </div>
    </div>
</div>




